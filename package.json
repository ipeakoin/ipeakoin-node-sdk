{
  "name": "@ipeakoin/ipeakoin-sdk",
  "version": "2.0.0",
  "description": "iPeakoin bass api",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint \"*.ts\" --fix",
    "release": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipeakoin/ipeakoin-node-sdk.git"
  },
  "keywords": [
    "iPeakoin",
    "Global Account",
    "Quantum Card",
    "Crypto Asset"
  ],
  "author": "iPeakoin Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ipeakoin/ipeakoin-node-sdk/issues"
  },
  "homepage": "https://github.com/ipeakoin/ipeakoin-node-sdk#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "superagent": "^8.0.9"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.12",
    "@types/superagent": "^4.1.17",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "prettier": "^1.19.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^8.6.2",
    "jest": "^27.4.7",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test/"
    ]
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
